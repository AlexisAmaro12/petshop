{{> navadmin}}
<div class="container">
    <div class="table-responsive">
        <table class="table table-bordered mt-4">
        <thead class="thead-dark">
            <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Cantidad</th>
            </tr>
        </thead>
        <tbody>
            {{#each detalles}}
            <tr>
            <td>{{this.nombre}}</td>
            <td>${{this.precio}}</td>
            <td>{{this.cantidad}}</td>
            </tr>
            {{/each}}
        </tbody>
        </table>
    </div>
</div>